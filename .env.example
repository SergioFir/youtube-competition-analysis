# Supabase credentials (get from Supabase dashboard)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here

# YouTube API (get from Google Cloud Console)
YOUTUBE_API_KEY=your-youtube-api-key-here

# Discovery mode: "polling" for local dev, "websub" for production
DISCOVERY_MODE=polling

# Intervals (in minutes/hours)
POLLING_INTERVAL_MINUTES=15
SNAPSHOT_WORKER_INTERVAL_MINUTES=5
BASELINE_UPDATE_HOURS=12
